<%= simple_form_for(@project) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :category %>
  <%= f.input :member %>
  <%= f.input :status, collection: Project.statuses %>
  <%= f.input :need %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
