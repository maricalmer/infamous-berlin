<div class="background-search">
  <%= render 'shared/navbar' %>
  <div class="container">
    <div class="mt-5 users-index-js">
      <!-- ^^^ wrong JS tag naming -->
      <%= simple_form_for :search, url: jobs_path, html: { class: "search-form-js" }, remote: true,  method: :get do |f| %>
        <div class="search-bar-search-page search-data" data-set="<%= @autocomplete_set.to_json %>">
          <label></label>
          <!-- ^^^ autocomplete dependency (change autocomplete.js??) -->
          <input type="text" name="query" placeholder="Enter a skill you need for your project" class="search-input-search-page refresh-input-js user-input-highlight-js", autocomplete="off">
          <button type="submit">
            <i class="fas fa-search search-btn-search-page"></i>
          </button>
        </div>
      <% end %>
    </div>
    <%= render "index_job", jobs: @jobs, query: params[:query] %>
  </div>
</div>
